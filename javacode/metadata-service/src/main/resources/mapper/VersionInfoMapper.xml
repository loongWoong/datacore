<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dataplatform.metadata.mapper.VersionInfoMapper">

    <select id="selectByTableId" resultType="com.dataplatform.metadata.entity.VersionInfo">
        SELECT * FROM metadata_version_info WHERE table_id = #{tableId}
        ORDER BY created_time DESC
    </select>

</mapper>